<h2>REGISTER</h2>

<form method="post" action="api/sessions/signup">
  <label for="name">NAME</label>
  <input type="text" placeholder="name" name="name" style="font-size:18pt;" />
  <br />
  <label for="email">E-MAIL</label>
  <input
    type="email"
    placeholder="email"
    name="email"
    style="font-size:18pt;"
  />
  <br />
  <label for="password">PASSWORD</label>
  <input
    type="password"
    placeholder="password"
    name="password"
    style="font-size:18pt;"
  />
  <br />
  <br />
  <button type="submit">Signup</button>
</form>